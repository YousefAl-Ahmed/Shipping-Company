    <h2 class="Register-text">Lost Packages</h2>

    <!-- if there is no packages show no  -->
    <% if (betweenDatesLostPackages.length==0) { %>
        <div class="no-packages">

            <h1c>No Lost packages found</h1>
        </div>

        <% } else {%>


            <div class="table-container">
                <table class="table">
                    <thead class="thead-dark">
                        <tr>
                            <th scope="col">Package ID</th>
                            <th scope="col">Package Name</th>
                            <th scope="col">Weight</th>
                            <th scope="col">Destination</th>
                            <th scope="col">Status</th>
                            <th scope="col">Dimensions</th>
                            <th scope="col">Insurance amount</th>
                            <th scope="col">Catagory</th>
                            <th scope="col">Delivery Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for (let i=0; i < betweenDatesLostPackages.length; i++) { %>
                            <tr>
                                <td>
                                    <%=betweenDatesLostPackages[i].package_id%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].package_name%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].weight%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].destination%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].status%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].dimensions%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].insurance_amount%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].catagory%>
                                </td>
                                <td>
                                    <%=betweenDatesLostPackages[i].final_delivery_date%>
                                </td>
                            </tr>
                            <% } %>
                </table>
            </div>
            <% } %>

                <hr>

                <!-- if there is no packages show no  -->

                <h2 class="Register-text">Delayed Packages</h2>
                <% if (betweenDatesDelayedPackages.length==0) { %>
                    <div class="no-packages">

                        <h1c>No delayed packages found</h1>
                    </div>

                    <% } else {%>

                        <div class="table-container">
                            <table class="table">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">Package ID</th>
                                        <th scope="col">Package Name</th>
                                        <th scope="col">Weight</th>
                                        <th scope="col">Destination</th>
                                        <th scope="col">Status</th>
                                        <th scope="col">Dimensions</th>
                                        <th scope="col">Insurance amount</th>
                                        <th scope="col">Catagory</th>
                                        <th scope="col">Delivery Date</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (let i=0; i < betweenDatesDelayedPackages.length; i++) { %>
                                        <tr>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].package_id%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].package_name%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].weight%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].destination%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].status%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].dimensions%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].insurance_amount%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].catagory%>
                                            </td>
                                            <td>
                                                <%=betweenDatesDelayedPackages[i].final_delivery_date%>
                                            </td>
                                        </tr>
                                        <% } %>
                            </table>
                        </div>
                        <% } %>

                            <hr>
                            <h2 class="Register-text">Delivered Packages</h2>
                            <% if (betweenDatesDeliveredPackages.length==0) { %>
                                <div class="no-packages">

                                    <h1c>No delivered packages found</h1>
                                </div>

                                <% } else {%>

                                    <div class="table-container">
                                        <table class="table">
                                            <thead class="thead-dark">
                                                <tr>
                                                    <th scope="col">Package ID</th>
                                                    <th scope="col">Package Name</th>
                                                    <th scope="col">Weight</th>
                                                    <th scope="col">Destination</th>
                                                    <th scope="col">Status</th>
                                                    <th scope="col">Dimensions</th>
                                                    <th scope="col">Insurance amount</th>
                                                    <th scope="col">Catagory</th>
                                                    <th scope="col">Delivery Date</th>
                                                    <th scope="col">Payment status</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% for (let i=0; i < betweenDatesDeliveredPackages.length; i++) { %>
                                                    <tr>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].package_id%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].package_name%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].weight%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].destination%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].status%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].dimensions%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].insurance_amount%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].catagory%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesDeliveredPackages[i].final_delivery_date%>
                                                        </td>
                                                        <td>
                                                            <%=betweenDatesPackages[i].payment_status%>
                                                    </tr>
                                                    <% } %>
                                        </table>
                                    </div>
                                    <% } %>


                                        <hr>

                                        <h2 class="Register-text">Packages Count</h2>
                                        <% if (catagory_count.length==0) { %>
                                            <div class="no-packages">

                                                <h1c>No packages found</h1>
                                            </div>

                                            <% } else {%>
                                                <div class="table-container">
                                                    <table class="table">
                                                        <thead class="thead-dark">
                                                            <tr>
                                                                <th scope="col">Catagory</th>
                                                                <th scope="col">Package Count</th>

                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <% for (let i=0; i < catagory_count.length; i++) { %>
                                                                <tr>
                                                                    <td>
                                                                        <%=catagory_count[i].catagory%>
                                                                    </td>
                                                                    <td>
                                                                        <%=catagory_count[i].packages_count%>
                                                                    </td>

                                                                    <% } %>
                                                    </table>
